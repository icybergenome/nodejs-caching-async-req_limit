  "version": "1.0.0",
  "description": "User Data API with Advanced Caching, Rate Limiting, and Asynchronous Processing",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "express",
    "typescript",
    "cache",
    "rate-limiting",
    "api"
